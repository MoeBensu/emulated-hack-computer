/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    Not (in=sel, out=invsel);
    And (a=sel, b=b, out=bAndsel);
    And (a=a, b=invsel, out=aAndinvsel);
    Or (a=bAndsel, b=aAndinvsel, out=out);

}